<div class="navbar navbar-default">
  <div class="navbar-header">
    <a id="white-brand" class="navbar-brand" href="/"><%= image_tag 'whealth.png', id: "nav_logo" %></a>
  </div>
  <ul id="white" class="nav navbar-nav pull-right">
    <% if @current_user %>
      <li><a class="btn default-btn" id="default-btn" href="/users/show">Hello, <%= @current_user.name%></a></li>
      <li><%= link_to "Edit Profile", users_edit_path(@current_user), class: 'btn default-btn', id: 'default-btn'%></li>
      <li><a class="btn default-btn" id="default-btn" href="/logout">Logout</a></li>
    <% else %>
      <li><a class="btn default-btn" id="default-btn" href="/login">Login</a></li>
      <li><a class="btn default-btn" id="default-btn" href="/signup">Signup</a></li>
    <% end %>
  </ul>
</div>